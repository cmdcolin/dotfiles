set nocompatible
filetype off

set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()
Plugin 'VundleVim/Vundle.vim'
Plugin 'editorconfig/editorconfig-vim'
call vundle#end()
filetype plugin indent on

nmap ; :
imap jj <Esc>
set syntax=on
set smartindent
set pastetoggle=<F2>
set tabstop=4
set expandtab
set sw=4
set clipboard+=unnamedplus
set viminfo='100,<1000,s100,h
